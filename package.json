{
  "name": "react-accelerator",
  "version": "1.0.0",
  "description": "Acclerator for bootstrapping React projects",
  "repository": "https://bitbucket.org/ZakButcher/react-accelerator/src/master/",
  "author": "Zak Butcher",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@storybook/addon-info": "^4.1.12",
    "@storybook/addon-options": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/next": "^8.0.1",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "@types/storybook__react": "^4.0.0",
    "@types/styled-components": "^4.1.6",
    "@zeit/next-typescript": "^1.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^7.0.0-0",
    "babel-loader": "^8.0.5",
    "babel-plugin-inline-react-svg": "^1.0.1",
    "babel-plugin-module-resolver": "^3.1.3",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-plugin-transform-md-import-to-string": "^1.0.6",
    "babel-preset-next": "^1.2.0",
    "babel-preset-react": "^6.24.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "module-resolver": "^1.0.0",
    "next": "^8.0.1",
    "node-fetch": "^2.3.0",
    "normalizr": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "polished": "^3.0.0",
    "prettier": "^1.16.4",
    "prop-types": "^15.6.2",
    "raw-loader": "^1.0.0",
    "react": "^16.7.0",
    "react-docgen-typescript-loader": "^3.0.1",
    "react-dom": "^16.7.0",
    "react-markdown": "^4.0.6",
    "storybook": "^1.0.0",
    "styled-components": "^4.1.3",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.1",
    "typescript-styled-plugin": "^0.13.0",
    "webpack": "^4.29.0"
  },
  "scripts": {
    "front": "next",
    "stories": "start-storybook -p 4000 -c .storybook",
    "up": "yarn run-p front stories",
    "clean-environment": "git clean -dfx && yarn",
    "lint": "tsc --noEmit && lint-staged",
    "pre-commit": "ts-node scripts/fail.master.commit.ts && yarn lint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn pre-commit"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{ts,tsx}": [
        "prettier --write",
        "git add"
      ],
      "**/*.{js,json}": [
        "prettier --write",
        "git add"
      ]
    }
  }
}
